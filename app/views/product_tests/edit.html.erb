<div id="container" class="container">
  <%= render :partial=>"shared/header" %>

  <div class="buffered">
    <section class="tb">
      <nav class="breadcrumb">
        <a href="<%= vendor_path %>">Certification Dashboard</a> &raquo;
        <a href="<%= vendor_path(@vendor) %>"><%= @vendor.name %></a> &raquo;
        <a href="<%= product_path(@product) %>"><%= @product.name %></a> &raquo;
        <span>Edit <%= @test.name %></span>
      </nav>
    </section>

    <section id="vendor" class="bordered buffered">
      <%= render :partial=>'product_test_details', :locals => { :submit_method => 'put', :submit_text => 'Save' } %>
    </section>
  </div>
</div>