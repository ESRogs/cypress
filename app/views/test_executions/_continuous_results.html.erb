 <table class="table">
         <thead>
           <tr>
             <th colspan="2" style="width:50%"></span>Continuous Variable Measures included in this test</th>
             <th>Patients</th>
             <th>Measure Population</th>
             <th>Observation Value</th>
         </thead>
         <tbody>
          <% measures.each do | measure| 

             expected_result = test_execution.expected_result(measure)
             reported_result = test_execution.reported_result(measure) %>
             <tr id='<%= "#{measure.key}" %>'>
              <td/>
               <td>
                 <a href="<%= product_test_measure_path(test_execution.product_test,measure,:execution => test_execution) %>">
                   <span style="font-weight:bold"><%= "#{measure.nqf_id} - #{measure.name} " %></span>
                   <%= " - #{measure.subtitle}" if measure.sub_id %>
                 </a>
               </td>
               <td>
                <%  reported_class = result_class(reported_result["population"],expected_result["population"]) %>
                <span class="<%=reported_class%>" ><%=reported_result["population"]%>/<%=expected_result["population"]%></span>
               </td>
               <td>
                <%  reported_class = result_class(reported_result["MSRPOPL"],expected_result["MSRPOPL"]) %>
                <span class="<%=reported_class%>" ><%=reported_result["MSRPOPL"]%>/<%=expected_result["MSRPOPL"]%></span>
               </td>

               <td>
                <%  reported_class = result_class(reported_result["OBSERV"],expected_result["OBSERV"]) %>
                <span class="<%=reported_class%>" ><%=reported_result["OBSERV"]%>/<%=expected_result["OBSERV"]%></span>
               </td>

             </tr>
           <% end %>
         </tbody>
     </table>