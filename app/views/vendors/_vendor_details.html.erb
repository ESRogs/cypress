<%= simple_form_for @vendor do |f| %>
  <%= f.input :name, :label => 'Name' %>
  <%= f.input :poc, :label => 'Point of contact' %>
  <%= f.input :tel, :label => 'Telephone' %>
  <%= f.input :proctor, :label => 'Proctor' %>
  <%= f.collection_check_boxes :measure_ids, @measures, :measure_id, :display_name %>
  <%= f.button :submit %>
<% end %>